                    </Items>
                </asp:Menu>--%>
            
        </div>
        <asp:ScriptManager ID="ScriptManager1" runat="server">
        </asp:ScriptManager>
        <div class="main">
            <asp:Label ID="lblGlobalMessage" runat="server" Text=""></asp:Label>
            <asp:ContentPlaceHolder ID="MainContent" runat="server" />
